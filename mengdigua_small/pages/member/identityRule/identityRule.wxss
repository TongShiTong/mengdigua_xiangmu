}

.show-message-total-two-pistion {
  position: absolute;
  transform: translateX(-50%);
}

.circle-message {
  display: flex;
  align-self: center;
  flex-wrap: wrap;
  justify-content: center;
  position: absolute;
  left: 50%;
  top: 50%;
  z-index: 5;
  transform: translate(-50%, -50%);
