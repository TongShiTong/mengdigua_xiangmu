					<view class='flex-r-s sign-total'>
						<block wx:for="{{detailData.meLabels}}" wx:key="index">
							<view class='theme-font-A-{{theme}} theme-lg-C-{{theme}}  each-sigin'>{{item.name}}</view>
						</block>
					</view>

					<view class='detail-bottom flex-r-b'>
						<view class='flex-row '>
							<view class='flex-row'>
								<image src="/images/gooddetail/postage.png" class="postage-img" mode="aspectFill" />
								<text class='postage' wx:if="{{ detailData.postage == 0||detailData.postage == null }}">包邮</text>
								<text class='postage' wx:else>运费￥{{ detailData.postage }}</text>
							</view>

						</view>
						<view class='flex-row'>
							<image src="/images/gooddetail/buy.png" class="buy-img" mode="aspectFill" />
							<view class='postage' wx:if="{{ detailData.deal_num == null }}">0人购买</view>
							<view class='postage' wx:else>{{ F1.numberDeal(detailData.deal_num)}}人购买</view>
						</view>
						<view class='flex-row '>
							<image src="/images/gooddetail/address.png" class="address-img" mode="aspectFill" />
							<view class='postage' wx:if="{{ detailData.shop.full_name }}">{{ F1.cityFilter(detailData.shop.full_name) }}</view>
							<view class='postage' wx:else>暂无</view>
						</view>
					</view>
				</view>
