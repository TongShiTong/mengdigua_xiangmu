				<!-- 店铺 -->
				<view 
          class='flex-row-s shop-info' 
          style="margin:0rpx;border-top:2rpx solid #f4f4f4;"
          wx:if="{{is_show_shop !=0}}">
					<view class='flex-row-f'>
						<image src="{{ detailData.shop.logo_url }}" class='shop-img' mode="aspectFill" />
						<view class='shop-right'>
							<view class='shop-name'>{{ detailData.shop.name }}</view>
							<view class='good-num'>共{{ detailData.shop.sum_item }}件商品</view>
						</view>
					</view>
					<view 
            class='flex-row-f' catchtap='jumpShopDetail' id="{{detailData.shop.id}}">
						<!-- <view class='shop-in theme-color-A-{{theme}}'>进入店铺</view> -->
						<view class='shop-in' style="color:rgba(153,153,153,1);">进入店铺</view>
						<image src="/images/small-right.png" class='shop-right-img' mode="aspectFill" />

					</view>
				</view>

				<!-- 评论 -->
				<view class='comment-head flex-row-s' style='margin-top:0rpx;border-top:2rpx solid #f4f4f4;' wx:if="{{ commentData.total_pages>0 }}">
					<view class='flex-row-f'>
						<view>买家评价({{ commentData.total_pages }})</view>
						<!-- <view class=' {{ commentData.total_pages>9?"comment-num":"comment-circel" }}'>{{ commentData.total_pages }}</view> -->
