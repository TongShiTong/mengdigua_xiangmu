							<view class="des-class">{{skillStatus==1?'距离拼团开始还有':skillStatus==2?'距砍拼团购结束仅剩':'拼团已经结束'}} </view>
							<view class="flex-r-s" wx:if="skillStatus!=3">
								<view class="time-day">{{time.day}}天</view>
								<view class="time-box">{{time.hour}}</view>
								<view class="color-white">:</view>
								<view class="time-box">{{time.minute}}</view>
								<view class="color-white">:</view>
								<view class="time-box">{{time.second}}</view>
							</view>

						</view>
						<view class="box-border" />
					</view>
				</block>
				<block wx:if="{{detailData.activity_type == 1}}">
					<view class="bargainTotal">
						<image class="bargain-image" mode="aspectFix" src="https://social-shop.oss-cn-hangzhou.aliyuncs.com/__liemi__/image/png/AWXZNTHK01245689_1568196744.png" />
						<view class="flex-c time-flex">
							<view class="des-class">{{skillStatus==1?'距离秒杀开始还有':skillStatus==2?'距砍秒杀购结束仅剩':'秒杀已经结束'}} </view>
							<view class="flex-r-s" wx:if="skillStatus!=3">
								<view class="time-day">{{time.day}}天</view>
								<view class="time-box">{{time.hour}}</view>
								<view class="color-white">:</view>
								<view class="time-box">{{time.minute}}</view>
								<view class="color-white">:</view>
								<view class="time-box">{{time.second}}</view>
							</view>

						</view>
						<view class="box-border" />
					</view>
				</block>
				<!-- 商品名字 价格 销量 -->
				<view class='detail-name' style="border-radius:25rpx 25rpx 0 0 ;">
					<view class="detail-header-wrap flex-r-s">
						<view class="new-good-price">
							<text style="font-size:28rpx;">￥</text>
							{{F1.moneyFormatTwo(detailData.activity_type!=3?detailData.price:detailData.bargainItem.end_price)}}
						</view>
						<view class="des-word" wx:if="{{detailData.activity_type==3}}">最低砍至</view>
						<view class="old-good-price">{{ F1.moneyFormatTwo(detailData.old_price)}}</view>
						<block wx:if="{{detailData.activity_type==3&&detailData.bargainItem.is_commission==1||detailData.activity_type==1}}">
							<view style="margin:0 6rpx;font-size:34rpx; font-family:DIN;">/</view>
							<!-- <view class="share-good-earn"> 赚{{ F1.moneyFormatTwo(detailData.share) }}</view> -->
						</block>
            <view style="margin-left:auto;" class="norm" wx:if="{{detailData.scale &&detailData.scale != ''}}">{{detailData.scale}}</view>
					</view>
					<view>
						<text class='abroad-title' wx:if="{{detailData.is_abroad==1}}">有机</text>
						{{ detailData.title }}</view>
					<!-- <text class='theme-font-A-{{theme}}' wx:if="{{detailData.is_abroad==1}}">【跨境购】</text> {{ detailData.title }}</view> -->
					<view class='sub-title'>{{ detailData.remark }}</view>
          <!-- <view style="text-align:right;">已售：{{detailData.deal_num}}</view> -->
					<view class='ys-price flex-row-f'>
