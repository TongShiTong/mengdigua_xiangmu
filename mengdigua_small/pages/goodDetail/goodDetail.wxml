<block wx:if="{{theme || theme===0}}">
	<navbar isTransparent='true' title="商品详情" wx:if="{{showBar}}" />
	<navbar title="商品详情" wx:if="{{!showBar}}" />
	<view class='back main-color ' wx:if="{{ shareCode }}" bindtap='jumpBack'>返回</view>
	<view class='top-tab-head {{ isChange===true?"top-head-white":"top-head-black"}}' style="top:{{barHeight}}px" wx:if="{{!showBar}}">
		<block wx:for="{{ topTab }}" wx:key="{{ index }}">
			<view class='top-tab' bindtap='selectTopTab' data-index='{{ index }}'>
				<view class='tab-text {{ index == currentIndex ?"active-tab-black":"active-tab-white" }}'>{{ item.title }}</view>
				<view class='line theme-lg-A-{{ theme }}' wx:if="{{ index == currentIndex }}" />
			</view>
		</block>
	</view>
	<swiper current="{{ currentIndex }}" style='height:100%;' bindchange="changeView">
		<!-- 商品 -->
		<swiper-item>
			<scroll-view
			 class="scroll-load-panel super"
			 style="padding-top:{{!showBar?'60rpx':'0px'}}"
			 bindscroll="scrollTop"
			 scroll-y="{{ isScroll }}"
			 bindscrolltolower="loadMoreMaterail"
			> 
        <view style="position:absolute;top:120rpx;right:40rpx;z-index:1;">
          <image
            src="/images/gooddetail/shopcar.png"
            style="width:52rpx;height:52rpx;margin-right:6rpx;"
            bindtap="toShopcar">
            </image>
          <image 
            src="{{isCollected?'/images/gooddetail/collect.png':'/images/gooddetail/uncollect.png'}}" 
            style="width:52rpx;height:52rpx;"
            bindtap="toCollect"></image>
        </view>
				<!-- 商品轮播 -->
				<swiper
				 class=''
				 indicator-dots="{{indicatorDots}}"
				 autoplay="{{autoplay}}"
				 style='height:750rpx;'
				>
					<swiper-item wx:if="{{short_video_url}}">
						<video
						 id='myVideo'
						 class='video-detail'
						 src="{{detailData.short_video_url}}"
						 custom-cache='false'
						 objectFit="cover"
						 poster="{{detailData.img_url}}"/>
					</swiper-item>
					<block wx:for="{{detailData.itemImgs }}" wx:key="{{ index }}">
						<swiper-item>
							<image
							 src="{{ item }}"
							 bindtap='previewBanner'
							 data-index="{{ index }}"
							 class="slide-image"
							 mode="aspectFill"/>
						</swiper-item>
					</block>
				</swiper>
				<view class='flex-row-s P30 spike-wrap spike-wrap2 ' wx:if="{{detailData.activity_type == 0&&detailData.is_seckill==1}}">
