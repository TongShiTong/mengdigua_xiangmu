				<!-- line16 -->
				<block 
          wx:if="{{detailData.activity_type == 2 && detailData.groupItem!='null'}}">
					<!-- <view class='bg16' /> -->
					<view style="width:750rpx;height:2rpx;border-top:2rpx solid #f4f4f4;" />
					<look-more
					 title='{{detailData.item_buy_item}}人正在拼团，可直接参与'
					 url="/pages/assemble/group/group?id={{goodId}}&join={{detailData.groupItem.is_join_group}}"
					 lookMore="true"
					 bgColor='#fff'
					 showType="5"
					/>
					<block wx:if="{{gropTeamList.list.length!=0}}">
						<swiper
						 style="height:{{gropTeamList.list.length==1?'68':'150'}}px"
						 circular="true"
						 vertical="true"
						 autoplay="true"
						>
							<block wx:for="{{ gropTeamList.list }}" wx:key="{{ index }}">
								<swiper-item wx:if="{{index%2!=1}}">
									<ingroup
									 bind:saveId="saveId"
									 data-alone="0"
									 data-go="true"
									 listData="{{F1.arraryFilter(gropTeamList.list,index,2)}}"
									 wx:if="{{gropTeamList}}"
									 userInfo="{{userInfo}}"
									/>
								</swiper-item>
							</block>
						</swiper>
					</block>
				</block>
				<!-- 店铺优惠券 -->
				<view class='flex-row-s  shop-coupon' catchtap='showCoupon' wx:if="{{detailData.activity_type==0&&couponListData.list.length>0}}">
					<view class='flex-row-f '>
						<view class='coupon-title' style="color:#000;">优惠</view>
						<view class='flex-row-f'>
							<view wx:for="{{ couponListData.list }}" wx:key="{{ index }}">
								<view wx:if="{{index<3}}">
									<view 
                    class='each-coupon theme-border-c-A-{{theme}}' 
                    style="color:#E50A35 !important;border:2rpx solid #E50A35 !important;"
                    wx:if="{{index<=3}}"
                    >满{{item.condition_num}}减{{item.discount_num}}</view>
									<view 
                    class='each-coupon theme-border-c-A-{{theme}}' 
                    wx:else>...</view>
								</view>

							</view>
						</view>
					</view>
					<image src='/images/small-right.png' class='small-right-img' mode="aspectFill" />
				</view>

				<view class='spec'>
					<view
					 class='wrap-all flex-row-s'
					 bindtap='buyNew'
					 data-alone="{{detailData.activity_type==2?0:2}}"
					 style="border-bottom:2rpx solid #f4f4f4;"
					>
						<view class='flex-row-f'>
							<view class='spec-text' style="color:#000;">选择规格</view>
							<view class='flex-row-f' style='width:500rpx'>
								<!-- <view class='spec-value spec-value-left' wx:if="{{ selectData  == '' }}">请选择规格</view> -->
								<!-- <text class='spec-label text-line1' >{{ propertyValue }}</text> -->
								<!-- <text class='spec-value spec-value-right text-line1' wx:else>{{ selectData.value_names }}</text> -->
								<text class='spec-value spec-value-right text-line1'>{{ selectData.value_names }}</text>
							</view>
						</view>
						<image src='/images/small-right.png' class='small-right-img' mode="aspectFill" />
					</view>
					<view class='wrap-all flex-row-s' bindtap='showService' data-alone="{{detailData.activity_type==2?0:2}}">
						<view class='flex-row-f'>
							<view class='spec-text' style="color:#000;">服务描述</view>
							<view
							 class='spec-value'
							 wx:if="{{ detailData.service }}"
							 bindtap='showService'
							 data-type="0"
							>{{ detailData.service }}
							</view>
							<view class='spec-value' wx:else>暂无服务描述</view>

						</view>
						<image src='/images/small-right.png' class='small-right-img' mode="aspectFill" />
					</view>
					<view style="padding:0 40rpx;" class="desp-wrap" />
