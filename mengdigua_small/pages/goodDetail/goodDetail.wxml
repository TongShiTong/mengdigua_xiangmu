		</block>
	</view>
	<!-- 服务描述modal -->
	<view
	 class='mask'
	 catchtouchmove="true"
	 wx:if="{{ serviceDes }}"
	 bindtap='closeServiceModal'
	/>
	<block>
		<view class='service-moadl' wx:if="{{ serviceDes }}">
			<view class='service-content flex-column'>
				<view class='flex-row-s service-head'>
					<view class='service-title'>{{launchGroup?contentTitle:'基础保障'}}</view>
					<image
					 src="/images/gooddetail/close.png"
					 class='close-img'
					 mode="aspectFill"
					 bindtap='closeServiceModal'
					/>
				</view>
				<block wx:if="{{!launchGroup}}">
					<scroll-view class="scroll-load-panel" scroll-y="true" style='max-height:800rpx;'>
						<view class='service-text' wx:for="{{ detailData.meNatures }}" wx:key="{{ index }}">
							<view class='natures'>
								<image src="{{ item.icon }}" class='icon-img' mode="aspectFill" />
								<view class='natures-right'>
									<view style='color:#000000;margin-bottom:20rpx'>{{ item.name }}</view>
									<view class='text-one'>{{ item.remark }}</view>
								</view>
							</view>
						</view>
					</scroll-view>
				</block>
				<block wx:else>
					<view class="flex-row-f" style='width:100%'>
						<image src="{{ detailData.img_url }}" class='good-img mr20' mode="aspectFill" />
						<rich hasp="0" rich="{{groupContent}}" style="width:100%" />
					</view>
				</block>
			</view>
			<view class='service-btn main-color theme-lg-A-{{theme}}' bindtap='closeServiceModal' wx:if="{{!launchGroup}}">完成</view>
			<view class='service-btn main-color theme-lg-A-{{theme}}' bindtap='spread' wx:else>立即推广</view>
		</view>
	</block>

	<modal wx:if="{{ isShowModal }}" isShowModal="{{ isShowModal }}" />
	<bind-phone wx:if="{{ isBindPhone }}" />
	<share showShare="{{ showShare }}" bind:shareSuccess="shareSuccess" shareImg="{{ shareImg.share_img }}" />

	<!--  -->
	<!-- 优惠券modal -->
	<view
	 class='mask'
	 catchtouchmove="true"
	 wx:if="{{couponModal}}"
	 catchtap='closeCouponModal'
	/>
	<view class='coupon-moadl' wx:if="{{couponModal}}" animation="{{animationData}}">
		<view class='coupon-content flex-column'>
			<scroll-view class="scroll" scroll-y="true" bindscrolltolower="loadMoreCoupon">
				<view class='coupon-title-two'>优惠券</view>
				<block wx:if="{{couponListShow}}">
					<view class='coupon-des'>可领优惠券</view>
					<view
					 wx:for="{{ couponListData.list }}"
					 wx:key="{{ index }}"
					 class='flex-column-s coupon-item'
					 data-coupon='{{ item }}'
					 bindtap='selectCoupon'
					 data-index='{{ index }}'
					>
						<view class='each-content flex-r-b' wx:if="{{item.is_accept==0}}">
							<view class='coupon-info '>
								<view class='coupon-info-top flex-r-s-b'>
									<view class='flex-r-s-b pirce-style'>
										<view class='theme-font-A-{{theme}}'>￥</view>
										<view class='spe-pirce theme-font-A-{{theme}}'>{{item.discount_num}}</view>
									</view>
									<view class='use-price'>满{{F1.moneyFormatTwo(item.condition_num)}}元可用</view>
								</view>
								<view class='coupon-info-time'>有效期:{{F1.timeSub(item.create_time)}} - {{F1.timeSub(item.update_time)}}</view>
							</view>
							<view class='coupon-type flex-column theme-lg-A-{{theme}}' catchtap='getShopCoupon' id="{{item.id}}">
								<view>领取</view>
								<view>店铺优惠卷</view>
							</view>
						</view>
					</view>
				</block>

				<view class='coupon-des'>已领取优惠卷</view>
				<view
				 wx:for="{{ couponListData.list }}"
				 wx:key="{{ index }}"
				 class='flex-column-s coupon-item'
				 data-coupon='{{ item }}'
				 bindtap='selectCoupon'
				 data-index='{{ index }}'
				>
					<view class='each-content flex-r-b' wx:if="{{item.is_accept==1}}">
						<view class='coupon-info '>
							<view class='coupon-info-top flex-r-s-b'>
								<view class='flex-r-s-b pirce-style'>
									<view class='theme-font-A-{{theme}}'>￥</view>
									<view class='spe-pirce theme-font-A-{{theme}}'>{{item.discount_num}}</view>
								</view>
								<view class='use-price'>满{{F1.moneyFormatTwo(item.condition_num)}}元可用</view>
							</view>
							<view class='coupon-info-time'>有效期:{{item.create_time}} - {{item.update_time}}</view>
						</view>
						<view class='coupon-type coupon-type-two flex-column theme-lg-C-{{theme}}'>
							<view>已领取</view>
							<view>店铺优惠卷</view>
						</view>

					</view>
				</view>
			</scroll-view>
		</view>
	</view>
</block>

<view wx:else>
	<page-loding/>
</view>

