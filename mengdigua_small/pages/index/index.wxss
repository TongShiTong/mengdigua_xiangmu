/**index.wxss**/

page {
  /* background-color: #fff; */
  height: auto;
}

.index-scroll-two {
  padding: 175rpx 0 98rpx 0 !important;
}

.user-head {
  height: 100rpx;
  position: fixed;
  left: 0;
  z-index: 33;
  width: 690rpx;
  padding: 0 30rpx;
}

.user-nickname {
  color: #c7c7c7;
  margin-left: 16rpx;
}

.serch-wrap {
  height: 75rpx;
  background-color: #fff;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 33;
  width: 100%;
}

.user-serch {
  width: 440rpx;
  height: 60rpx;
  padding: 0 30rpx;
  background: rgba(246, 244, 245, 1);
  border-radius: 30rpx;
}

.serch-text {
  color: #c2c2c2;
}

.head-img {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}

.banner-wrap {
  width: 690rpx;
  padding: 15rpx 30rpx;
  background: #fff;
}

.swiper {
  height: 380rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.back-swiper {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.index-scroll {
  padding: 175rpx 0 140rpx 0;
}

/* tab */

.tab-wrap {
  height: 100rpx;
  background-color: #fff;
  position: fixed;
  left: 0;
  z-index: 33;
  width: 100%;
}

.tab {
  padding: 0 20rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  display: inline-block;
  position: relative;
  color: #fff;
}

.nav-active {
  color: #fff;
}

.nav-un {
  color: #666;
}

.floor-content {
  margin-top: 0;
}

/* 秒杀 */

.skill-scroll {
  height: 108rpx;
  padding: 6rpx 30rpx 0 30rpx;
  background-color: #fff;
  border-top: 1px solid #eee;
}

.skill {
  width: 690rpx;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #f4f4f4;
  background: #fff;
}

.skill:last-child {
  border-bottom: transparent;
}

.skill-tab {
  padding-bottom: 10rpx;
  border-radius: 8rpx;
  width: 132rpx;
  text-align: center;
  display: inline-block;
  color: #b2b2b2;
  position: relative;
  margin: 10rpx;
  box-sizing: border-box;
  /* overflow: hidden; */
}

skill-tab:after {
  content: "";
  position: absolute;
  right: -12rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 2rpx;
  height: 24rpx;
  background: #eee;
}

.skill-tab2::after {
  content: "";
  position: absolute;
  right: -12rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 2rpx;
  height: 24rpx;
  background: #eee;
}

.skill-time {
  font-weight: 700;
  font-size: 28rpx;
  color: #b2b2b2;
}

.skill-time-active {
  color: #b52902;
}

.skill-title {
  font-size: 20rpx;
}

.skill-title-active {
  color: #b52902;
}

.skill-title-two {
  color: #fff;
}

.skill-deal-num {
  color: #999;
  font-size: 24rpx;
}

.skill-line {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 4rpx;
  border-radius: 3rpx;
  background-color: #b52902;
  z-index: 10;
}

.skill-img {
  width: 690rpx;
  height: 332rpx;
  border-radius: 10rpx;
}

.skill-good-name {
  color: #333;
  margin-top: 10rpx;
}

.skill-good-remark {
  padding: 20rpx 0;
  font-size: 24rpx;
  color: #c0c0c0;
}

.is-skill {
  width: 86rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  background: rgba(32, 31, 30, 0);
  border: 1px solid #b52902;
  border-radius: 6rpx;
  color: #b52902;
  font-size: 22rpx;
  margin-right: 20rpx;
}

.seckill-price {
  font-size: 30rpx;
}

.share-price {
  position: relative;
  height: 29rpx;
  border-radius: 15rpx;
  text-align: right;
}

.earn {
  position: absolute;
  width: 34rpx;
  height: 34rpx;
  color: #fff;
  background-color: #b52902;
  text-align: center;
  line-height: 34rpx;
  border-radius: 50%;
  left: 0;
  top: 50%;
  font-size: 22rpx;
  transform: translateY(-50%);
}

.earn-price {
  margin-left: 26rpx;
  height: 29rpx;
  line-height: 29rpx;
  padding: 0 12rpx 0 20rpx;
  font-size: 24rpx;
  border-radius: 0 18rpx 18rpx 0;
}

.list-btn {
  border-radius: 6rpx;
  text-align: center;
}

.buy {
  border: 1px solid #1d1e1f;
  margin-right: 10rpx;
  width: 108rpx;
  height: 56rpx;
  text-align: center;
  line-height: 56rpx;
  color: #fff;
}

.extend {
  color: #fff;
  width: 110rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
}

.class-wrap {
  width: 110rpx;
  height: 100rpx;
  background-color: #fff;
  /* box-shadow: -8rpx 0 20rpx 2rpx rgba(39, 40, 41, 0.2); */
  box-shadow: -8rpx 0 20rpx 2rpx rgba(255, 255, 255, 0.1);
}

.class-img {
  width: 32rpx;
  height: 32rpx;
}

.tab-line {
  position: absolute;
  bottom: 2rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 40%;
  height: 6rpx;
  border-radius: 3rpx;
  background-color: #fff;
}

.white7 {
  color: #b2b2b2;
  font-weight: 500;
}

.nav {
  flex-wrap: wrap;
  padding: 40rpx 30rpx 0 30rpx;
}

.column {
  position: relative;
  width: 20%;
  margin-bottom: 40rpx;
}

.nav-img {
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
}

.nav-title {
  color: #1f1f1e;
  margin-top: 8rpx;
}

.good-img {
  width: 100%;
  height: 100%;
  display: block;
}

.floor-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  background: #fff;
  flex: 1;
}

.floor {
  width: 100%;
  height: 100%;
}

.modal-img {
  width: 590rpx;
  height: 800rpx;
}

.modal-po {
  position: relative;
}

.modal-amx {
  position: fixed;
  z-index: 9999;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.index-cancel-img {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: -60rpx;
}

/* 固定秒杀 */

.spike-fixed {
  position: fixed;
  /* position: -webkit-sticky; */
  width: 100%;
  margin-top: -16rpx;
  z-index: 100;
}

/* 拼团 */

.group-top-scroll {
  /* height: 106rpx; */
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  width: 100%;
  background: #fff;
}

.look-more {
  position: -webkit-sticky;
  position: sticky;
  top: 1000px;
  width: 100%;
  z-index: 99999;
}

.onde {
  position: -webkit-sticky;
  position: sticky;
  top: 100px;
}


